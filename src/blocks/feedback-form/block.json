{
    "$schema" : "https://schemas.wp.org/trunk/block.json",
    "apiVersion" : 1,
    "name" : "jabp/feedback-form",
    "title" : "Feedback form",
    "category" : "theme",
    "description" : "Evaluate content with a quick feedback form.",
    "keywords" : [
        "feedback",
        "form"
    ],
    "attributes" : {
        "content" : {
            "type" : "string",
            "source" : "text",
            "selector" : ".jabp-feedback-form__heading",
            "__experimentalRole" : "content"
        },
        "level" : {
            "type" : "number",
            "default" : 2,
            "enum" : [
                2,
                3,
                4,
                5,
                6
            ]
        },
        "showLevelAs" : {
            "type" : "number",
            "default" : 2,
            "enum" : [
                1,
                2,
                3,
                4,
                5,
                6
            ]
        },
        "style" : {
            "type" : "number",
            "default" : 1,
            "enum" : [
                1,
                2,
                3
            ]
        },
        "buttonStyle" : {
            "type" : "string",
            "default" : "default",
            "enum" : [
                "default",
                "outline",
                "outline-fill"
            ]
        },
        "hideButtonLabel" : {
            "type" : "boolean",
            "default" : false
        }
    },
    "icon" : "feedback",
    "style" : "jabp-feedback-form",
    "viewScript" : [
        "jabp-feedback-form"
    ],
    "editorStyle" : [
        "jabp-feedback-form-editor"
    ],
    "editorScript" : "file:./index.js",
    "supports" : {
        "anchor" : true,
        "className" : false,
        "customClassName" : false,
        "html" : false,
        "multiple" : false
    },
    "textdomain" : "jabp",
    "version" : "1.0.0",
    "render" : "file:./render.php"
}
