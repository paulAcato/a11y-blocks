{
    "$schema" : "https://schemas.wp.org/trunk/block.json",
    "name" : "a11y-blocks/heading",
    "title" : "A11Y Blocks Heading",
    "description" : "Heading description",
    "category" : "text",
    "attributes" : {
        "content" : {
            "type" : "string",
            "source" : "text",
            "selector" : "h1, h2, h3, h4, h5, h6"
        },
        "tag" : {
            "type" : "number",
            "default" : 2
        },
        "showAs" : {
            "type" : "number"
        },
        "srOnly" : {
            "type" : "boolean",
            "default" : false
        }
    },
    "example" : {
        "attributes" : {
            "content" : "This shows the default heading content."
        }
    },
    "editorScript": "file:./index.js",
    "style" : [
        "a11y-blocks-heading-block"
    ],
    "editorStyle" : [
        "a11y-blocks-heading-block-editor"
    ],
    "supports" : {
        "anchor" : true,
        "className" : false,
        "customClassName" : false,
        "html" : false
    },
    "textdomain" : "a11y-blocks",
    "version" : "1.0.0",
    "render": "file:./render.php"
}
